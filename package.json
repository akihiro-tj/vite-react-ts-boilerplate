{
  "name": "vite-react-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/akihiro-tj/vite-react-ts-boilerplate.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint src --fix",
    "fix:stylelint": "stylelint \"src/**/*.{css,scss}\" --fix",
    "fix:prettier": "prettier --write *.{md,json}",
    "latest": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^28.1.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sanitize.css": "^13.0.0",
    "sass": "^1.58.1",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  }
